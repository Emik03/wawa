<Project Sdk="Microsoft.NET.Sdk">
    <PropertyGroup>
        <FileVersion>3</FileVersion>
        <Nullable>annotations</Nullable>
        <TargetFramework>net35</TargetFramework>
        <DefineConstants>$(DefineConstants);WAWA</DefineConstants>
        <WeaverConfiguration>
            <Weavers>
                <Equals />
                <InfoOf />
                <InlineIL />
                <ToString />
                <NullGuard Mode="Implicit" />
                <Substitute />
                <Virtuosity />
                <JetBrainsAnnotations />
                <LocalsInit Default="false" />
                <Caseless StringComparison="InvariantCulture" />
                <InlineMethod />
                <Divorce />
                <Absence />
            </Weavers>
        </WeaverConfiguration>
    </PropertyGroup>
    <ItemGroup>
        <Compile Remove="$(Morsels)Source/*.cs" />
        <PackageReference Remove="GlobalUsingsAnalyzer" />
        <ProjectReference Include="..\wawa.IO\wawa.IO.csproj" />
        <ProjectReference Include="..\wawa.DDL\wawa.DDL.csproj" />
        <ProjectReference Include="..\wawa.Unity\wawa.Unity.csproj" />
        <ProjectReference Include="..\wawa.Optionals\wawa.Optionals.csproj" />
        <Compile Include="../Polyfills/MoreAttributes.cs" Link="Source/References/System/Diagnostics/MoreAttributes.cs" />
    </ItemGroup>
</Project>
